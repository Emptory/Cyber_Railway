# 数据库配置
DATABASE_URL=mysql+pymysql://username:password@rm-xxxxx.mysql.rds.aliyuncs.com:3306/cyber_railway

# Flask配置
SECRET_KEY=your-super-secret-key-here
FLASK_ENV=production

# 阿里云配置
ALIYUN_ACCESS_KEY_ID=your_access_key_id
ALIYUN_ACCESS_KEY_SECRET=your_access_key_secret

# OSS配置（可选，用于大文件存储）
OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
OSS_BUCKET_NAME=cyber-railway-data

# Redis配置（可选，用于缓存）
REDIS_URL=redis://r-xxxxx.redis.rds.aliyuncs.com:6379/0
